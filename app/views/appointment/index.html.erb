<h1><%= @doctor.name %>: Appointments</h1>
<%= link_to 'Add Appointment', new_doctor_appointment_path(@doctor) %>
<hr />

<h2>Doctor</h2>
<ul>
  <% @doctors.each do |doctor| %>
    <li>
      <%= doctor.user.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appointment_path(@doctor, doctor), method: :delete %>
    </li>
  <% end %>
</ul>
<h2>Job</h2>
<ul>
  <% @jobs.each do |job| %>
    <li>
      <%= ta.user.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appointment_path(@doctor, job), method: :delete %>
    </li>
 <% end %>
</ul>
<h2>Patient</h2>
<ul>
  <% @patients.each do |patient| %>
    <li>
      <%= patient.patient.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appointment_path(@doctor, patient), method: :delete %>
   </li>
 <% end %>
</ul>